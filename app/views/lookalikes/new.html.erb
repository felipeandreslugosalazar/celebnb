<h1>Create your Look-alike</h1>

<%= form_for(@lookalike) do |f| %>
<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= f.label :celeb_name %>
  <%= f.text_field :celeb_name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.file_field :photo %>
  <%= f.submit %>
<% end %>

<%= link_to "Return to all lookalikes", lookalikes_path %>
